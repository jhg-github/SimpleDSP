  float dst[FILTER_DEC_DEST_BLOCK_SIZE];
  res = arm_fir_decimate_init_f32 ( &filter_dec, FILTER_DEC_NTAPS, FILTER_DEC_M, filter_dec_coeffs, filter_dec_state, FILTER_DEC_SRC_BLOCK_SIZE );
  if( res != ARM_MATH_SUCCESS ){
    while(1); // Failed
  }

#define FILTER_BAND_NTAPS       (51)
#define FILTER_BAND_BLOCK_SIZE  (FILTER_DEC_DEST_BLOCK_SIZE)
#define FILTER_BAND_STATE_SIZE  (FILTER_BAND_NTAPS+FILTER_BAND_BLOCK_SIZE-1)

  arm_fir_instance_f32 filter_band;
  const float filter_band_coeffs[FILTER_BAND_NTAPS] = {-8.862911e-04f,-2.119672e-03f,-1.306148e-03f,1.623597e-03f,3.965636e-03f,2.324467e-03f,-2.560232e-03f,-5.154549e-03f,-2.245936e-03f,1.430184e-03f,-2.077625e-18f,-2.155339e-03f,5.114292e-03f,1.782324e-02f,1.352965e-02f,-1.889317e-02f,-4.973749e-02f,-3.126857e-02f,3.785901e-02f,8.874635e-02f,5.052678e-02f,-5.603605e-02f,-1.212726e-01f,-6.409985e-02f,6.625163e-02f,1.339629e-01f,6.625163e-02f,-6.409985e-02f,-1.212726e-01f,-5.603605e-02f,5.052678e-02f,8.874635e-02f,3.785901e-02f,-3.126857e-02f,-4.973749e-02f,-1.889317e-02f,1.352965e-02f,1.782324e-02f,5.114292e-03f,-2.155339e-03f,-2.077625e-18f,1.430184e-03f,-2.245936e-03f,-5.154549e-03f,-2.560232e-03f,2.324467e-03f,3.965636e-03f,1.623597e-03f,-1.306148e-03f,-2.119672e-03f,-8.862911e-04f};
  float filter_band_state[FILTER_BAND_STATE_SIZE];
  float signal_band[FILTER_BAND_BLOCK_SIZE];
  arm_fir_init_f32 ( &filter_band, FILTER_BAND_NTAPS, filter_band_coeffs, filter_band_state, FILTER_BAND_BLOCK_SIZE );

  while(1){
    LL_GPIO_SetOutputPin(LD2_GPIO_Port, LD2_Pin);
    arm_fir_decimate_f32 ( &filter_dec, src, dst, FILTER_DEC_SRC_BLOCK_SIZE );
    arm_fir_f32 ( &filter_band, dst, signal_band, FILTER_BAND_BLOCK_SIZE );
    LL_GPIO_ResetOutputPin(LD2_GPIO_Port, LD2_Pin);
    HAL_Delay(100);
  }

}

