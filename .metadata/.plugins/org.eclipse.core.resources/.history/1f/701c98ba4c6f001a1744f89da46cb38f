  float dst[FILTER_DEC_DEST_BLOCK_SIZE];
  res = arm_fir_decimate_init_f32 ( &filter_dec, FILTER_DEC_NTAPS, FILTER_DEC_M, filter_dec_coeffs, filter_dec_state, FILTER_DEC_SRC_BLOCK_SIZE );
  if( res != ARM_MATH_SUCCESS ){
    while(1); // Failed
  }

#define FILTER_LOW_NTAPS       (51)
#define FILTER_LOW_BLOCK_SIZE  (FILTER_DEC_DEST_BLOCK_SIZE)
#define FILTER_LOW_STATE_SIZE  (FILTER_LOW_NTAPS+FILTER_LOW_BLOCK_SIZE-1)

  arm_fir_instance_f32 filter_low;
  const float filter_low_coeffs[FILTER_LOW_NTAPS] = {-8.821272e-04f,-6.519373e-04f,-2.717764e-04f,3.378294e-04f,1.219692e-03f,2.313547e-03f,3.410162e-03f,4.150526e-03f,4.084251e-03f,2.784717e-03f,-2.067864e-18f,-4.196670e-03f,-9.300376e-03f,-1.435156e-02f,-1.802114e-02f,-1.880441e-02f,-1.529752e-02f,-6.506200e-03f,7.877503e-03f,2.729529e-02f,5.028940e-02f,7.463855e-02f,9.765066e-02f,1.165660e-01f,1.289988e-01f,1.333335e-01f,1.289988e-01f,1.165660e-01f,9.765066e-02f,7.463855e-02f,5.028940e-02f,2.729529e-02f,7.877503e-03f,-6.506200e-03f,-1.529752e-02f,-1.880441e-02f,-1.802114e-02f,-1.435156e-02f,-9.300376e-03f,-4.196670e-03f,-2.067864e-18f,2.784717e-03f,4.084251e-03f,4.150526e-03f,3.410162e-03f,2.313547e-03f,1.219692e-03f,3.378294e-04f,-2.717764e-04f,-6.519373e-04f,-8.821272e-04f};
  float filter_low_state[FILTER_LOW_STATE_SIZE];
  float signal_low[FILTER_LOW_BLOCK_SIZE];
  arm_fir_init_f32 ( &filter_low, FILTER_LOW_NTAPS, filter_low_coeffs, filter_low_state, FILTER_LOW_BLOCK_SIZE );

  while(1){
    LL_GPIO_SetOutputPin(LD2_GPIO_Port, LD2_Pin);
    arm_fir_decimate_f32 ( &filter_dec, src, dst, FILTER_DEC_SRC_BLOCK_SIZE );
    arm_fir_f32 ( &filter_low, dst, signal_low, FILTER_LOW_BLOCK_SIZE );
    LL_GPIO_ResetOutputPin(LD2_GPIO_Port, LD2_Pin);
    HAL_Delay(100);
  }

}


